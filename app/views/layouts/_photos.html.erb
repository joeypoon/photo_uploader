<% @photos.each do |photo| %>

  <div class="photo" style="float: left;">

    <%= image_tag photo.name.url(:thumb) %>

    <div class="clearfix"></div>

    Tags: <% photo.tag_list.each do |tag| %>

      <%= link_to tag, tag_path(tag: tag) %>

    <% end %>

  </div>

<% end %>

<div class="clearfix"></div>

<%= paginate @photos %>
